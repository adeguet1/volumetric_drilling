<package format="3">
  <name>volumetric_drilling</name>
  <version>0.0.0</version>
  <description>
  Volumetric Drilling plugin for AMBF
  </description>
  <maintainer email="anton.deguet@jhu.edu">Anton Deguet</maintainer>
  <license>xxx</license>
  <url>xxx</url>

  <buildtool_depend condition="$ROS_VERSION == 1">catkin</buildtool_depend>

  <build_depend condition="$ROS_VERSION == 1">roscpp</build_depend>
  <build_depend condition="$ROS_VERSION == 1">roslib</build_depend>
  <build_depend condition="$ROS_VERSION == 1">geometry_msgs</build_depend>
  <build_depend condition="$ROS_VERSION == 1">ambf_server</build_depend>
  <build_depend condition="$ROS_VERSION == 1">message_generation</build_depend>

  <exec_depend condition="$ROS_VERSION == 1">roscpp</exec_depend>
  <exec_depend condition="$ROS_VERSION == 1">roslib</exec_depend>
  <exec_depend condition="$ROS_VERSION == 1">geometry_msgs</exec_depend>
  <exec_depend condition="$ROS_VERSION == 1">ambf_server</exec_depend>
  <exec_depend condition="$ROS_VERSION == 1">message_runtime</exec_depend>

  <buildtool_depend condition="$ROS_VERSION == 2">ament_cmake</buildtool_depend>
  <depend condition="$ROS_VERSION == 2">rclcpp</depend>
  <depend condition="$ROS_VERSION == 2">ambf_server</depend>
  <depend condition="$ROS_VERSION == 2">geometry_msgs</depend>
  <depend condition="$ROS_VERSION == 2">volumetric_drilling_msgs</depend>
  <depend condition="$ROS_VERSION == 2">message_generation</depend>

  <export>
    <build_type condition="$ROS_VERSION == 2">ament_cmake</build_type>
  </export>

</package>
